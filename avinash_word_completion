#include <stdio.h>
#include <string.h>

// Driver function
int main()
{

	// initializing variables
	char st[] = "GeeksforGeeks";
	//char ch;
	char* val;
    //printf("enter the word:");
    //scanf("%c",ch);
	/*val = strrchr(st, ch);

	printf("String after last %c is : %s \n",ch, val);*/

	char ch2;
    printf("enter the value of ch2");
    scanf("%c",ch2);

	// Use of strrchr()
	// returns null
	// test for null
	val = strrchr(st, ch2);

	printf("String after last %c is : %s ",ch2, val);

	return (0);
}#include <stdio.h>
